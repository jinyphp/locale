{
    "name": "jiny/locale",
    "description": "jinyPHP Locale",
    "keywords": [
        "jiny", "jinyphp", "jinyerp",
        "framwork", "laravel", "locale"
    ],
    "homepage": "http://www.jinyphp.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com",
            "homepage": "http://www.jinyphp.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3|^8.0",
        "livewire/livewire": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\Locale\\": "src/",
            "Jiny\\Locale\\Database\\Seeds\\": "database/seeds/"
        },
        "files": [
            "src/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Locale\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Locale\\JinyLocaleServiceProvider"
            ]
        }
    }
}
